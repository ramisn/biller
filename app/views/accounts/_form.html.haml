= semantic_form_for @account do |f|
  .in_fld
    = f.inputs do
      = f.input :display_name
      = f.input :email
      = f.input :phone
    .form-actions
      = f.buttons do
        - link_to "Cancel", :back, :class => "float-right"
        = f.commit_button  :button_html => {:class=>"btn btn-primary primary_crt", :id => "acc_invitation"}  
