= link_to "Add Account", new_account_path(), :class=>"btn btn-primary btn-sub"
%h1 Accounts
%form.search{:action => accounts_path, :method => "get"}
%input{:name => "q", :type => "serach", :value => params[:q]}/
%input.btn{:type => "submit", :class => "btn btn-primary"}/
= raw page_entries_info @accounts
%table{ :class => "zebra-striped table table-striped"}
  %thead
    %tr
      %th.header.blue Vendor Name
      %th.blue.header Email
      %th.blue.header Phone
  %tbody
    = render @accounts
= paginate @accounts
